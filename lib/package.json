{
  "name": "gilmore",
  "version": "1.0.0",
  "author": "Tim Carry (@pixelastic)",
  "description": "Toolbox for git and GitHub",
  "keywords": [
    "git"
  ],
  "repository": "pixelastic/gilmore",
  "homepage": "https://projects.pixelastic.com/gilmore/",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "*.js",
    "methods/"
  ],
  "exports": {
    ".": "./main.js"
  },
  "main": "main.js",
  "dependencies": {
    "firost": "5.3.0",
    "golgoth": "3.0.0",
    "parse-github-repo-url": "1.4.1"
  },
  "scripts": {
    "build": "cd ../ && ./scripts/build",
    "build:prod": "cd ../ && ./scripts/build-prod",
    "cms": "cd ../ && ./scripts/cms",
    "compress": "cd ../ && ./scripts/compress",
    "lint": "cd ../ && ./scripts/lint",
    "lint:fix": "cd ../ && ./scripts/lint-fix",
    "release": "cd ../ && ./scripts/release",
    "serve": "cd ../ && ./scripts/serve",
    "test": "cd ../ && ./scripts/test",
    "test:watch": "cd ../ && ./scripts/test-watch"
  }
}
